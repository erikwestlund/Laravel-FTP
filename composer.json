    "keywords": ["ftp", "laravel", "l6", "l7", "service provider"],
    "license": "MIT",
    "authors": [{
        "name": "Harish Anchu",
        "email": "harishanchu@gmail.com"
    }],
    "require": {
        "php": ">=8.1.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "autoload": {
        "psr-0": {
            "Anchu\\Ftp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
